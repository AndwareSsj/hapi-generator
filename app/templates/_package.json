{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "lab -c -L",
    "test-cover": "lab -c -r html -o ./test/artifacts/coverage.html && open ./test/artifacts/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "<%= gitRepo %>"
  },
  "author": "<%= author %><% if (authorEmail) { %> <<%= authorEmail %>><% } %>",
  "license": "<%= license %>",
  "bugs": {
    "url": "<%= bugsUrl %>"
  },
  "homepage": "<%= homepageUrl %>",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "confidence": "3.x.x",
    "debug": "^2.6.0",
    "glue": "4.x.x",
    "hapi": "15.x.x",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-swagger": "^7.5.0",
    "heredoc": "^1.3.1",
    "hoek": "4.x.x",
    "inert": "^4.1.0",
    "jade": "1.x.x",
    "moment": "^2.17.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.1",
    "tv": "^6.1.0",
    "vision": "4.x.x",
    "visionary": "6.x.x"
  },
  "devDependencies": {
    "code": "3.x.x",
    "lab": "11.x.x",
    "nodemon": "1.x.x"
  }
}
