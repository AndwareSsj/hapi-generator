{
  "name": "<%= pluginName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "test": "lab -c -L",
    "test-cover": "lab -c -r html -o ./test/artifacts/coverage.html && open ./test/artifacts/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "<%= gitRepo %>"
  },
  "author": "<%= author %><% if (authorEmail) { %> <<%= authorEmail %>><% } %>",
  "license": "<%= license %>",
  "bugs": {
    "url": "<%= bugsUrl %>"
  },
  "homepage": "<%= homepageUrl %>",
  "devDependencies": {
    "hapi": "^11.x.x",
    "code": "^1.x.x",
    "lab": "^6.x.x"
  }
}
