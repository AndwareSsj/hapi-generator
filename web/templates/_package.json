{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js server",
    "test": "./node_modules/lab/bin/lab -c -L",
    "test-cover": "./node_modules/lab/bin/lab -c -r html -o ./test/artifacts/coverage.html && open ./test/artifacts/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "<%= gitRepo %>"
  },
  "keywords": <%= keywords %>,
  "author": "<%= author %><% if (authorEmail) { %> <<%= authorEmail %>><% } %>",
  "license": "<%= license %>",
  "bugs": {
    "url": "<%= bugsUrl %>"
  },
  "homepage": "<%= homepageUrl %>",
  "dependencies": {
    "confidence": "^1.0.0",
    "glue": "^2.0.0",
    "hapi": "^8.2.0",
    "hoek": "^2.11.1",
    "jade": "^1.5.0",
    "visionary": "^2.0.0"
  },
  "devDependencies": {
    "code": "^1.3.0",
    "lab": "^5.4.0",
    "nodemon": "^1.3.7"
  }
}
